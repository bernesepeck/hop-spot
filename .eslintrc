{
  "extends": [
    "plugin:prettier/recommended"
  ],
  "ignorePatterns": ["common/kolibri/", "service/firebase/"],
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}